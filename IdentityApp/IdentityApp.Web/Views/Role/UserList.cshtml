@using IdentityApp.Db.Entities;
@model IEnumerable<User>

@{
    ViewBag.Title = "User List";
}

<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.UserName</td>
            <td>@user.Email</td>
            <td>
                <a class="btn btn-sm btn-primary" asp-action="Update" asp-route-userid="@user.Id">Access rights</a>
            </td>
        </tr>
    }
</table>